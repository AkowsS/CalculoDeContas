{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "target": "es2017",
    "jsx": "react",
    "lib": ["es2017", "dom"],
    "moduleResolution": "node",
    "noEmit": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true
  },
  "exclude": ["node_modules", "babel.config.js", "metro.config.js"]
}
  
